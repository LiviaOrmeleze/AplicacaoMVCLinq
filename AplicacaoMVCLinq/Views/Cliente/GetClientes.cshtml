@model IEnumerable<AplicacaoMVCLinq.Models.Cliente>
@{
    ViewData["Title"] = "Listar Clientes";
}
<h1>Listar Clientes</h1>
<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Nome</th>
            <th>Email</th>
            <th>Telefone</th>
            <th>DataNasc</th>
        </tr>
    </thead>

    <!-- Laço de Repetição foreach para listar todos os elementos da lista de aluno-->
    <tbody>
        @foreach (var cliente in Model)
        {

            <tr>
                <td>@cliente.Id</td>
                <td>@cliente.Nome</td>
                <td>@cliente.Email</td>
                <td>@cliente.Telefone</td>
                <td>@cliente.DataNasc</td>


            </tr>

        }
    </tbody>
</table>
